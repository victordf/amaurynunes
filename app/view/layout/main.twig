<script>
    function validaForm(form){
        $("#"+form+" [required]").each(function(){
            var campo = $(this);
            var valor = campo.val();
            switch (campo.attr('type')){
                default:
                case 'text':
                    if(valor == ''){
                        swal('O campo \''+campo.attr('name')+'\' é obrigatório.');
                        return false;
                    }
                    break;

                case 'email':
                    if(valor.indexOf('@') == -1){
                        swal('Email inválido.');
                        return false;
                    }
                    if(valor == ''){
                        swal('O campo '+campo.attr('name')+' é obrigatório.');
                        return false;
                    }
                    break;

            }
        });
        return true;
    }
</script>

